{
  "name": "cardspro-bo-web",
  "version": "0.1.0",
  "description": "CardsPro Back Office",
  "author": "Vladimir Krylov <vkrylov@iqcard.ru>",
  "homepage": "https://github.com/IQCard/iqcard-web/cardspro-bo-web",
  "license": "WTFPL",
  "scripts": {
    "clean": "rimraf doc typings coverage dist",
    "clean:dist": "rimraf dist",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config config/webpack.prod.js --profile --colors --display-error-details --display-cached --bail",
    "server": "npm run server:dev:hmr --",
    "server:dev": "webpack-dev-server --inline --profile --colors --watch --display-error-details --display-cached  --content-base src/",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "webpack-dev-server --config config/webpack.prod.js",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "lint": "./node_modules/.bin/tslint src/**/*.ts",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "protractor",
    "e2e:live": "protractor --elementExplorer",
    "test": "karma start",
    "ci": "npm run e2e && npm run test",
    "docs": "typedoc --options typedoc.json --exclude **/*.spec.ts ./src/",
    "typings-install": "typings install",
    "start": "npm run server --",
    "postinstall": "npm run typings-install"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "^0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/platform-server": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "core-js": "^2.4.0",
    "rxjs": "5.0.0-beta.10",
    "zone.js": "~0.6.12"
  },
  "devDependencies": {
    "angular2-hmr": "^0.8.1",
    "autoprefixer": "^6.3.7",
    "awesome-typescript-loader": "==1.1.1",
    "codelyzer": "0.0.25",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "parse5": "^2.1.5",
    "postcss-loader": "^0.9.1",
    "raw-loader": "0.5.1",
    "request": "^2.73.0",
    "rimraf": "^2.5.3",
    "script-loader": "^0.7.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.1",
    "ts-node": "^0.9.3",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.0",
    "typings": "~1.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.14.0",
    "yargs": "^4.8.0"
  },
  "peerDependencies": {
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IQCard/iqcard-web.git"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
